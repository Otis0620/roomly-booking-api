{
   "name": "roomly-booking-api",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/jest": "^29.5.14",
      "@types/node": "^16.11.10",
      "@types/supertest": "^6.0.2",
      "@typescript-eslint/eslint-plugin": "^8.25.0",
      "@typescript-eslint/parser": "^8.25.0",
      "eslint": "^9.21.0",
      "eslint-config-prettier": "^10.0.2",
      "eslint-plugin-jest": "^28.11.0",
      "eslint-plugin-prettier": "^5.2.3",
      "jest": "^29.7.0",
      "prettier": "^3.5.2",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.6",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "body-parser": "^1.19.1",
      "dotenv": "^16.4.7",
      "express": "^4.17.2",
      "mysql2": "^3.12.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "migration:generate": "npm run typeorm -- migration:generate ./src/migration/${npm_config_name} -d ./src/data-source.ts",
      "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
      "migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
      "db:seed": "ts-node ./src/seeds/seed.ts",
      "db:clear": "ts-node ./src/seeds/clear-seeds.ts",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "format": "prettier --write \"src/**/*.ts\""
   }
}
