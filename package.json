{
   "name": "roomly-booking-api",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "start": "ts-node -r tsconfig-paths/register src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/${npm_config_name} -d ./src/data-source.ts",
      "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
      "migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
      "db:seed": "ts-node ./src/seeds/seed.ts",
      "db:clear": "ts-node ./src/seeds/clear-seeds.ts",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "format": "prettier --write \"src/**/*.ts\""
   },
   "devDependencies": {
      "@eslint/js": "^9.22.0",
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^5.0.0",
      "@types/jest": "^29.5.14",
      "@types/joi": "^17.2.3",
      "@types/node": "^16.11.10",
      "@types/passport": "^1.0.17",
      "@types/passport-local": "^1.0.38",
      "@types/supertest": "^6.0.2",
      "@typescript-eslint/eslint-plugin": "^8.25.0",
      "@typescript-eslint/parser": "^8.25.0",
      "eslint": "^9.22.0",
      "eslint-config-prettier": "^10.0.2",
      "eslint-import-resolver-typescript": "^3.8.3",
      "eslint-plugin-import": "^2.31.0",
      "eslint-plugin-jest": "^28.11.0",
      "eslint-plugin-prettier": "^5.2.3",
      "globals": "^16.0.0",
      "jest": "^29.7.0",
      "prettier": "^3.5.2",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.6",
      "ts-node": "10.9.1",
      "typescript": "^4.8.4",
      "typescript-eslint": "^8.26.0"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "body-parser": "^1.19.1",
      "dotenv": "^16.4.7",
      "express": "^4.17.2",
      "helmet": "^8.0.0",
      "inversify": "^7.0.1",
      "joi": "^17.13.3",
      "mysql2": "^3.12.0",
      "passport": "^0.7.0",
      "passport-local": "^1.0.0",
      "reflect-metadata": "^0.1.14",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "0.3.20"
   }
}
